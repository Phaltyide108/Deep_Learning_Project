ping = EarlyStopping(monitor='val_loss', patience=10, restore_best_weights=True)\n\n# Train the model\nprint(\"Training the model...\")\nhistory = model.fit(\n    train_dataset,\n    steps_per_epoch=train_generator.samples // batch_size,\n    validation_data=validation_dataset,\n    validation_steps=validation_generator.samples // batch_size,\n    epochs=15,  # Adjust epochs as needed\n    callbacks=[early_stopping]\n)\n\n# Save the trained model\nmodel.save(\"/kaggle/working/wbc_classification_model.h5\")\nprint(\"Model saved to Kaggle.\")\n\n# Evaluate the model on the test set\nprint(\"Evaluating the model on test set...\")\ntest_loss, test_accuracy = model.evaluate(test_dataset, steps=test_generator.samples // batch_size)\nprint(f\"Test Accuracy: {test_accuracy * 100:.2f}%\")\n\n# Plot Training and Validation Accuracy\nplt.plot(history.history['accuracy'], label='Training Accuracy')\nplt.plot(history.history['val_accuracy'], label='Validation Accuracy')\nplt.title('Training and Validation Accuracy')\nplt.xlabel('Epoch')\nplt.ylabel('Accuracy')\nplt.legend()\nplt.show()","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-03-15T10:34:40.202096Z","iopub.execute_input":"2025-03-15T10:34:40.202476Z","execution_failed":"2025-03-15T10:40:27.415Z"}},"outputs":[{"name":"stdout","text":"Found 11222 images belonging to 9 classes.\nFound 10742 images belonging to 9 classes.\nFound 5285 images belonging to 9 classes.\nTraining the model...\nEpoch 1/15\n\u001b[1m350/350\u001b[0m \u001b[32m━━━━━━━━━━━━━━━━━━━━\u001b[0m\u001b[37m\u001b[0m \u001b[1m70s\u001b[0m 169ms/step - accuracy: 0.4691 - loss: 1.9231 - val_accuracy: 0.2567 - val_loss: 3.0350\nEpoch 2/15\n\u001b[1m350/350\u001b[0m \u001b[32m━━━━━━━━━━━━━━━━━━━━\u001b[0m\u001b[37m\u001b[0m \u001b[1m39s\u001b[0m 93ms/step - accuracy: 0.7333 - loss: 0.8821 - val_accuracy: 0.5250 - val_loss: 2.2110\nEpoch 3/15\n\u001b[1m350/350\u001b[0m \u001b[32m━━━━━━━━━━━━━━━━━━━━\u001b[0m\u001b[37m\u001b[0m \u001b[1m31s\u001b[